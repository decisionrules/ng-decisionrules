{
  "name": "@decisionrules/ng-decisionrules",
  "description": "A simple Angular library that allows you to easily connect to Decisionrules.io from your web application.",
  "version": "1.1.0",
  "author": "Decisionrules.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/decisionrules/ng-decisionrules.git"
  },
  "bugs": {
    "url": "https://github.com/decisionrules/ng-decisionrules/issues"
  },
  "homepage": "https://github.com/decisionrules/ng-decisionrules",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "build-library": "ng build ng-decisionrules",
    "pack-lib": "cd dist/ng-decisionrules && npm pack",
    "build-pack": "ng build ng-decisionrules && cd dist/ng-decisionrules && npm pack",
    "copy-readme": "copy README.md ..\\..\\README.md /y",
    "build": "npm run copy-readme && npm version patch && ng build --prod",
    "publish": "cd ../../dist/ng-decisionrules && npm publish",
    "build-prod": "ng build --prod"
  },
  "peerDependencies": {
    "@angular/common": "^10.1.6",
    "@angular/core": "^10.1.6",
    "rxjs": "^6.6.3"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
